<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ShapeAppearanceOverlay.AiANG.Rounded" parent="">
        <item name="cornerSize">8dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.AiANG.Sharp" parent="">
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
    </style>

    <style name="Widget.AiANG.Button.IconButton.Borderless" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:layout_width">?minTouchTargetSize</item>
        <item name="android:layout_height">?minTouchTargetSize</item>
        <item name="android:insetLeft">4dp</item>
        <item name="android:insetRight">4dp</item>
        <item name="android:insetTop">4dp</item>
        <item name="android:insetBottom">4dp</item>
        <item name="android:minHeight">?minTouchTargetSize</item>
        <item name="android:minWidth">?minTouchTargetSize</item>
        <item name="android:padding">0dp</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">0dp</item>
        <item name="iconSize">24dp</item>
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay.AiANG.Discorect</item>
    </style>

    <style name="Widget.AiANG.Button.Outlined" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">?minTouchTargetSize</item>
        <item name="android:insetLeft">0dp</item>
        <item name="android:insetRight">0dp</item>
        <item name="android:insetTop">4dp</item>
        <item name="android:insetBottom">4dp</item>
        <item name="android:letterSpacing">0.00714286</item>
        <item name="android:textAllCaps">false</item>
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay.AiANG.Discorect</item>
    </style>

    <style name="Widget.AiANG.Spacer" parent="">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="Widget.AiANG.Button.IconButton.Outlined" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">?minTouchTargetSize</item>
